<div class="container">
  <div id="login-page" class="row">
    <div class="col s4 offset-s4 z-depth-4 card-panel login-panel">
      <form class="login-form" method="post" action="/new_user">

        <div class="login-panel-header input-field col s12 center">
          <p class="center login-form-text login-subheader">Qualidade de Software</p>
        </div>

        <div class="login-panel-body">
          <div class="margin">
            <div class="input-field col s12">
              <i class="material-icons prefix">sentiment_satisfied</i>
              <input id="name" name="name" type="text">
              <label for="name" class="center-align">
            </div>
          </div>

          <div class="margin">
            <div class="input-field col s12">
              <i class="material-icons prefix">person_outline</i>
              <input id="nickname" name="nickname" type="text">
              <label for="nickname" class="center-align">
            </div>
          </div>

          <div class="margin">
            <div class="input-field col s12">
              <i class="material-icons prefix">phone</i>
              <input id="phone_number" name="phone_number" type="text">
              <label for="phone_number" class="center-align">
            </div>
          </div>

          <div class="margin">
            <div class="input-field col s12">
              <i class="material-icons prefix">lock_outline</i>
              <input id="password" name="password" type="password">
              <label for="password" class="center-align">
            </div>
          </div>

          <div class="login-btn input-field col s12" style="margin-bottom: 20px !important;">
            <button type="submit" class="btn waves-effect waves-light material-white col s12">Registrar</button>
          </div>
        </div>

      </form>
    </div>
  </div>
</div>
